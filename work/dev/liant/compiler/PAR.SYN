Begin

    Language  "Parse-Tool"
    Nickname  "parse"
    Extension "par"

    PSletters "_"

    Endline "--"

    Matching "'"

    Casesensitivity On
    Indenting On

    Argmark "#"
    Continuation "$"

    Builtin "@"

    Larglist "("
    Rarglist ")"
    Argbreak ","

    Sescape "\"
    Lescape "<"
    Rescape ">"

End
