  2 reserved_1_7		bit (1),

  /* BYTE TWO */

  2 reserved_2_0		bit (1),
  2 reserved_2_1		bit (1),
  2 reserved_2_2		bit (1),
  2 reserved_2_3		bit (1),
  2 reserved_2_4		bit (1),
  2 reserved_2_5		bit (1),
  2 reserved_2_6		bit (1),
  2 reserved_2_7		bit (1);

